{
    "version": 2,
    "public": false,
    "github": {
        "enabled": false
    },
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python",
            "config": { "excludeFiles": "cache/**" }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ]
}
